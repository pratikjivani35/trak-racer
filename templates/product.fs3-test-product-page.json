        "844e4e46-0897-43d2-9b8d-06260df6ccc9": {
          "type": "image",
          "settings": {
            "content_max_width": 780,
            "mobile_text_position": "place-self-start-center text-center",
            "desktop_text_position": "sm:place-self-start sm:text-start",
            "subheading": "We've designed the stand to be compatible with all the popular steering wheel, shifter, and pedal sets on the market, including Logitech, Thrustmaster, and Fanatec.  Additionally, pre-drilled mounting holes are available for all major brands, ensuring easy installation.",
            "title": "Maximum Compatibility",
            "heading_tag": "h4",
            "button_text": "",
            "button_link": "",
            "text_color": "#ffffff",
            "button_background": "#ffffff",
