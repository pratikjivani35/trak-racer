    "4c633010-91ac-4cc3-a8a9-ad6ff192ed45": {
      "type": "slideshow",
      "blocks": {
        "template--19236073701664__4c633010-91ac-4cc3-a8a9-ad6ff192ed45-1685584167944544db-0": {
          "type": "image",
