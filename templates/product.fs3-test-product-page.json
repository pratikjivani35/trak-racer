            "content_max_width": 780,
            "mobile_text_position": "place-self-start-center text-center",
            "desktop_text_position": "sm:place-self-start-end sm:text-end",
            "subheading": "You have the freedom to raise or lower the wheel stand to your desired height. This allows you to find the most comfortable position that suits your racing style.",
            "title": "Height and Angle Adjustments",
            "heading_tag": "h4",
            "button_text": "",
            "button_link": "",
            "text_color": "#ffffff",
