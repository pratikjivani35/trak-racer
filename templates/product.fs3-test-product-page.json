        },
        "1d5dbb33-f7e0-42d1-9487-92822115a20f": {
          "type": "image",
          "settings": {
            "content_max_width": 780,
            "mobile_text_position": "place-self-start-center text-center",
            "desktop_text_position": "sm:place-self-start-end sm:text-end",
            "subheading": "The stand is 650mm wide, providing a firm stance when racing. It works seamlessly with most fixed and office\/race chairs, ensuring stability and comfort during your sessions.",
            "title": "Office Chair Compatibility",
            "heading_tag": "h4",
            "button_text": "",
            "button_link": "",
