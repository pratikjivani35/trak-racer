        "template--19236073701664__4c633010-91ac-4cc3-a8a9-ad6ff192ed45-1685584167944544db-1": {
          "type": "image",
          "settings": {
            "content_max_width": 780,
            "mobile_text_position": "place-self-start-center text-center",
            "desktop_text_position": "sm:place-self-start sm:text-start",
            "subheading": "Our compact design enables the stand to fold away in a matter of seconds. It's easy to store, so you can maximize your limited space while still having a great racing setup whenever you need it.",
            "title": "Easy Storage",
            "heading_tag": "h4",
            "button_text": "",
            "button_link": "",
            "text_color": "#ffffff",
